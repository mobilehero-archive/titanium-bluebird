		"@geek/spawn": "^0.0.3",
		"@geek/spinner": "^1.0.2",
		"@geek/tld": "^2.0.1-1",
		"@nativeloop/mobile": "^1.0.9",
		"@titanium/appcelerator": "^6.0.12",
		"@titanium/authentication": "^1.0.24",
		"@titanium/authentication-mock": "^1.0.7",
		"@titanium/authentication-oauth": "^1.0.27",
		"@titanium/bluebird": "^3.7.17",
